@model IEnumerable<mvcPet.Entities.Precio>

@{
    ViewBag.Title = "IndexByService";
}

<h2>IndexByService</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { id = ViewBag.IdTipoServicio })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FechaDesde)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaHasta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Valor)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FechaDesde)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaHasta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Valor)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}



</table>

<div>
    @Html.ActionLink("Volver", "Index", "TipoServicio", null, new { @class = "btn btn-primary"})
</div>